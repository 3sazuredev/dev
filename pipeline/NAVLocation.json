{
	"name": "NAVLocation",
	"properties": {
		"activities": [
			{
				"name": "ForEachCompany",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.Company",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "CopyDataNAV",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "RelationalSource",
									"query": {
										"value": "@concat('company=',item(),'&$select=',variables('Columns'))",
										"type": "Expression"
									}
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".csv"
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "ODataNAVLocation",
									"type": "DatasetReference"
								}
							],
							"outputs": [
								{
									"referenceName": "DataLakeStoreRaw",
									"type": "DatasetReference"
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"Company": {
				"type": "array",
				"defaultValue": [
					"3StepIT_TEST",
					"CRONUS Finland Oy"
				]
			}
		},
		"variables": {
			"Entity": {
				"type": "String",
				"defaultValue": "Location"
			},
			"Columns": {
				"type": "String",
				"defaultValue": "*"
			}
		},
		"folder": {
			"name": "NAV Extract"
		},
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}