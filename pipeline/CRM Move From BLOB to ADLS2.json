{
	"name": "CRM Move From BLOB to ADLS2",
	"properties": {
		"activities": [
			{
				"name": "Copy as CSV from BLOB to ADLS2",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "MergeFiles"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "CRMTemporaryBLOB",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DataLakeStoreRawCRM",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Delete from Temporary BLOB",
				"type": "Delete",
				"dependsOn": [
					{
						"activity": "Copy as JSON from BLOB to ADLS2",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "CRMTemporaryBLOB",
						"type": "DatasetReference"
					},
					"enableLogging": false
				}
			},
			{
				"name": "Copy as JSON from BLOB to ADLS2",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Copy as CSV from BLOB to ADLS2",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "AzureBlobFSSink",
						"copyBehavior": "MergeFiles"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "CRMTemporaryBLOB_JSON",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DataLakeStoreRawCRM_JSON",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "CRM Extract"
		},
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}